{
  "name": "backend",
  "version": "0.0.2",
  "description": "A Prebaked Medusa 2.0 backend",
  "author": "Rasmus Puls",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node src/scripts/postBuild.js",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "ib": "node ./scripts/init-backend.js",
    "start": "pnpm run ib && cd .medusa/server && medusa start --verbose",
    "dev": "medusa develop",
    "email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002",
    "search:rebuild": "medusa exec ./src/scripts/reindex-meilisearch.ts",
    "search:check": "medusa exec ./src/scripts/check-meilisearch-sync.ts",
    "search:sync-settings": "medusa exec ./src/scripts/sync-meilisearch-settings.ts",
    "search:sync": "pnpm run search:sync-settings && pnpm run search:rebuild",
    "search:prepare": "pnpm run search:sync && pnpm run search:check",
    "lint": "eslint --ext .ts,.tsx src"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "^2.12.1",
    "@medusajs/admin-shared": "^2.12.1",
    "@medusajs/cli": "^2.12.1",
    "@medusajs/core-flows": "^2.12.1",
    "@medusajs/dashboard": "^2.12.1",
    "@medusajs/draft-order": "^2.12.1",
    "@medusajs/framework": "^2.12.1",
    "@medusajs/medusa": "^2.12.1",
    "@medusajs/notification-sendgrid": "^2.12.1",
    "@medusajs/payment-stripe": "^2.12.1",
    "@medusajs/utils": "^2.12.1",
    "@medusajs/workflow-engine-redis": "^2.12.1",
    "@mikro-orm/core": "6.4.16",
    "@mikro-orm/knex": "6.4.16",
    "@mikro-orm/migrations": "6.4.16",
    "@mikro-orm/postgresql": "6.4.16",
    "@react-email/components": "1.0.0-tailwindv4.6",
    "@rokmohar/medusa-plugin-meilisearch": "^1.3.5",
    "awilix": "^8.0.1",
    "csv-parse": "^5.6.0",
    "medusajs-launch-utils": "^0.0.18",
    "minio": "^8.0.3",
    "pg": "^8.13.1",
    "resend": "4.0.1",
    "stripe": "^19.1.0",
    "ts-node": "^10.9.2",
    "ulid": "^2.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@medusajs/test-utils": "^2.12.1",
    "@mikro-orm/cli": "6.4.16",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.38.0",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.46.2"
  },
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=22.0.0 <26",
    "pnpm": ">=10.19.0"
  },
  "pnpm": {
    "overrides": {
      "@mikro-orm/*": "6.4.16"
    }
  }
}
